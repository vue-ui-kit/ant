{
  "name": "@vue-ui-kit/ant",
  "version": "1.0.0",
  "description": "Vue3 UI Kit based on Ant Design",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/es/index.js"
    },
    "./scss": "./src/packages/styles/index.scss"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vue-tsc --noEmit --skipLibCheck & vite",
    "build": "ts-patch install && vite build",
    "prepack": "rimraf dist && yarn build",
    "prepare": "husky",
    "lint:lint-staged": "lint-staged",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\""
  },
  "keywords": [
    "vue3",
    "ui kit",
    "ant design",
    "ant",
    "antdv"
  ],
  "author": {
    "name": "adoin",
    "email": "adoin@qq.com"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "uuid": "^10.0.0"
  },
  "peerDependencies": {
    "@ant-design/icons-vue": ">=7.0.0",
    "ant-design-vue": ">=4.0.0",
    "vue": ">=3.2.0"
  },
  "license": "GPL",
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@ant-design/icons-vue": ">=7.0.0",
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@types/lodash-es": "^4.17.12",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "ant-design-vue": "^4.2.3",
    "esbuild": "^0.23.1",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "sass": "^1.77.8",
    "ts-patch": "^3.2.1",
    "typescript-transform-paths": "^3.5.0",
    "vite": "^5.4.2",
    "vite-plugin-dts": "^4.1.0",
    "vue": "^3.4.38"
  }
}
